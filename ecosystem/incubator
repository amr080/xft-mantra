<script>
  // Ensure gsap and ScrollTrigger are initialized
  gsap.registerPlugin(ScrollTrigger);

  //Progress bullets active states
  function checkWidth() {
    const bar = document.querySelector(".progress-track-active");
    const point = document.querySelector(".progress-point.is--2");
    const point2 = document.querySelector(".progress-point.is--3");
    const cards = document.querySelectorAll(".hor-scroll__card");

    // Get the computed style of the bar
    const barStyle = window.getComputedStyle(bar);
    const barWidth = parseFloat(barStyle.width);
    const parentWidth = bar.parentElement.clientWidth;

    // Check if the bar width is 50% or more of its parent
    if (barWidth >= parentWidth * 0.5) {
      point.classList.add("is-active");
      cards[0].classList.remove("is--active");
      cards[1].classList.add("is--active");
    } else {
      point.classList.remove("is-active");
      cards[1].classList.remove("is--active");
      cards[0].classList.add("is--active");
    }

    // Check if the bar width is 100% of its parent
    if (barWidth >= parentWidth * 0.98) {
      point2.classList.add("is-active");
      cards[1].classList.remove("is--active");
      cards[2].classList.add("is--active");
    } else {
      point2.classList.remove("is-active");
      cards[2].classList.remove("is--active");
      if (!point.classList.contains("is-active")) {
        cards[0].classList.add("is--active");
      }
    }
  }

  // Run the checkWidth function when the window is resized
  window.addEventListener("resize", checkWidth);

  // Initial check when the page is loaded
  window.addEventListener("DOMContentLoaded", (event) => {
    checkWidth();
  });

  //ScrollTrigger
  $(".scroll_horizontal_wrap").each(function (index) {
    let wrap = $(this);
    let inner = $(this).find(".scroll_horizontal_inner");
    let track = $(this).find(".scroll_horizontal_track");

    // set section height
    function setScrollDistance() {
      wrap.css("height", "calc(" + track.outerWidth() + "px + 100vh)");
    }
    setScrollDistance();
    ScrollTrigger.refresh();
    window.addEventListener("resize", setScrollDistance);

    // create main horizontal scroll timeline
    let tl = gsap.timeline({
      scrollTrigger: {
        trigger: wrap,
        start: "top top",
        end: "bottom bottom",
        scrub: true,
        onUpdate: checkWidth, // Call checkWidth on every update
        // markers: true,
      },
      defaults: { ease: "none" },
    });

    tl.to(track, { xPercent: -100 });
    tl.from(".progress-track-active", { width: "0%" }, "<");

    // get container left position
    function containerLeft() {
      return inner.offset().left + "px";
    }

    // get container right position
    function containerRight() {
      return inner.offset().left + inner.innerWidth() + "px";
    }

    let tl2 = gsap.timeline({
      scrollTrigger: {
        trigger: $(this).find(".scroll_horizontal_hero_wrap"),
        containerAnimation: tl,
        start: "left " + containerLeft(),
        end: "right " + containerLeft(),
        scrub: true,
        // markers: true,
      },
      defaults: { ease: "none" },
    });
  });

  const formulaSwiper = new Swiper(".swiper", {
    loop: false,
    spaceBetween: 24,
    allowTouchMove: true,
    followFinger: true,
    slidesPerView: "auto",
  });
</script></body></html>