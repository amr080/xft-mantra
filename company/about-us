<script>
  document.addEventListener("DOMContentLoaded", () => {
    gsap.registerPlugin(MotionPathPlugin, ScrollTrigger);

    function handleImageAnimation() {
      const containers = document.querySelectorAll('[data-svg="container"]');
      containers.forEach((container) => {
        const images = container.querySelectorAll('[data-svg="img"]');
        let animationTriggered = false;

        // Initial state for images
        gsap.set(images, { y: "30%" });

        // ScrollTrigger for container
        ScrollTrigger.create({
          trigger: container,
          start: "top 80%",
          onEnter: () => {
            if (!animationTriggered) {
              gsap.to(images, {
                x: "0%",
                y: "0%",
                duration: 1.55,
                ease: "power2.inOut",
                stagger: 0.3, // Adjust stagger timing as needed
              });
              animationTriggered = true;
            }
          },
        });
      });
    }

    function initImgPaths(container) {
      const path = container.querySelector('[data-svg="path"] path');
      const items = container.querySelectorAll('[data-svg="item"]');
      const shouldFlip = container.getAttribute("data-svg-flip") === "true";
      const pathLength = path.getTotalLength();
      const numItems = items.length;
      const itemSpacing = pathLength / numItems;

      gsap.set(items, {
        xPercent: -50,
        yPercent: -50,
        transformOrigin: "50% 50%",
      });

      items.forEach((item, index) => {
        const startPosition = (index * itemSpacing) / pathLength;
        const endPosition = ((index + 1) * itemSpacing) / pathLength;
        const images = item.querySelectorAll('[data-svg="img"]');

        gsap.set(item, {
          motionPath: {
            path: path,
            align: path,
            autoRotate: shouldFlip ? 180 : true,
            alignOrigin: [0.5, 0.5],
            start: startPosition,
            end: endPosition,
          },
        });

        const tl = gsap.timeline({ repeat: -1 });
        const autoplaySpeed = 1;

        tl.to(item, {
          duration: 30,
          ease: "none",
          motionPath: {
            path: path,
            align: path,
            autoRotate: shouldFlip ? 180 : true,
            alignOrigin: [0.5, 0.5],
            start: startPosition,
            end: startPosition + 1,
          },
          onUpdate: () => {
            const position = gsap.getProperty(item, "motionPath.start");
            if (position >= 1) {
              gsap.set(item, { motionPath: { start: position - 1 } });
            }
          },
        });
        /*
      ScrollTrigger.create({
        trigger: container,
        onUpdate: (self) => {
          const velocity = self.getVelocity();
          const direction = velocity < 0 ? -1 : 1;
          const speedAdjustment = Math.abs(velocity) / 500;
          tl.timeScale(
            Math.max(autoplaySpeed + direction * speedAdjustment, 0.1),
          );
        },
        onLeave: () => tl.timeScale(autoplaySpeed),
        onEnterBack: () => tl.timeScale(autoplaySpeed),
      }); */
      });
    }

    function initContainers() {
      const containers = document.querySelectorAll('[data-svg="container"]');
      containers.forEach((container) => {
        gsap.killTweensOf(container.querySelectorAll('[data-svg="item"]'));
        initImgPaths(container);
      });
    }

    let lastWidth = window.innerWidth;

    function callAfterResize(func, delay) {
      let dc = gsap.delayedCall(delay || 0.2, func).pause();
      let handler = () => {
        const currentWidth = window.innerWidth;
        if (currentWidth !== lastWidth) {
          lastWidth = currentWidth;
          dc.restart(true);
        }
      };
      window.addEventListener("resize", handler);
      return handler; // in case you want to window.removeEventListener() later
    }

    callAfterResize(() => {
      ScrollTrigger.refresh();
      initContainers();
    }, 0.2);

    initContainers();
    handleImageAnimation();
  });

  // ------------------------------------ the function
  // mantra section scroll
  function initSectionScroll() {
    const container = document.querySelector('[data-mc="container"]');
    const items = document.querySelectorAll('[data-mc="item"]');

    items.forEach((item, index) => {
      const content = item.querySelector('[data-mc="item-content"]');
      gsap.set(content, { scale: 1, filter: "blur(0px)", y: 0 });

      ScrollTrigger.create({
        trigger: item,
        start: "top top",
        end: "bottom top",
        onEnter: () => {
          if (index > 0) {
            const prevContent = items[index - 1].querySelector(
              '[data-mc="item-content"]',
            );
            const scale = 0.95;
            const yOffset = "-2rem";
            gsap.to(prevContent, {
              scale: scale,
              filter: "blur(1px)",
              y: yOffset,
              duration: 0.3,
            });
          }
        },
        onEnterBack: () => {
          if (index > 0) {
            const prevContent = items[index - 1].querySelector(
              '[data-mc="item-content"]',
            );
            const scale = 0.95;
            const yOffset = "2rem";
            gsap.to(prevContent, {
              scale: scale,
              filter: "blur(1px)",
              y: yOffset,
              duration: 0.3,
            });
          }
        },
        onLeave: () => {
          if (index < items.length - 1) {
            gsap.to(content, {
              scale: 1,
              filter: "blur(0px)",
              y: 0,
              duration: 0.3,
            });
          }

          for (let i = 0; i < index; i++) {
            const prevContent = items[i].querySelector(
              '[data-mc="item-content"]',
            );
            const scale = 0.95 - (index - i) * 0.05;
            const yOffset = `-${(index - i + 1) * 2}rem`;
            gsap.to(prevContent, {
              scale: scale,
              filter: "blur(1px)",
              y: yOffset,
              duration: 0.3,
            });
          }
        },
        onLeaveBack: () => {
          gsap.to(content, {
            scale: 1,
            filter: "blur(0px)",
            y: 0,
            duration: 0.3,
          });

          if (index > 0) {
            const prevContent = items[index - 1].querySelector(
              '[data-mc="item-content"]',
            );
            gsap.to(prevContent, {
              scale: 1,
              filter: "blur(0px)",
              y: 0,
              duration: 0.3,
            });
          }
        },
      });
    });
  }

  // do something after resize
  function callAfterResize(func, delay) {
    let dc = gsap.delayedCall(delay || 0.2, func).pause();
    const handler = () => dc.restart(true);
    window.addEventListener("resize", handler);
    return handler;
  }

  // ------------------------------------ launch control
  // call all functions and account for resizing
  document.addEventListener("DOMContentLoaded", (event) => {
    gsap.registerPlugin(ScrollTrigger);

    // section scroll function
    initSectionScroll();

    // refresh ScrollTrigger instances after a resize event
    const refreshScrollTrigger = () => {
      ScrollTrigger.refresh();
    };

    // call the refresh function after a resize event with a slight delay
    callAfterResize(refreshScrollTrigger, 0.2);
  });

</script></body></html>