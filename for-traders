<script>
  document.addEventListener('DOMContentLoaded', function() {
    // Set a timeout to move the .globe element after a delay
    setTimeout(function() {
      // Select the .globe element
      const globeElement = document.querySelector('.globe');

      // Select the .language-option.active-language element
      const activeLanguageElement = document.querySelector('.language-option.active-language');

      // Check if both elements exist
      if (globeElement && activeLanguageElement) {
        // Select the first 'a' element inside the .language-option.active-language element
        const linkElement = activeLanguageElement.querySelector('a');

        // If the link element exists, insert the .globe element before it
        if (linkElement) {
          activeLanguageElement.insertBefore(globeElement, linkElement);
        } else {
          // If there's no 'a' element, append the .globe element to the activeLanguageElement
          activeLanguageElement.appendChild(globeElement);
        }
      } else {
        console.error('Either .globe or .language-option.active-language element is not found.');
      }
    }, 200); // Delay of 200 milliseconds
  });
</script>

<script>
  // Function to handle the appending of the .weglot-container.wg-default element
  function handleResponsiveWeglot() {
    // Select the .weglot-container.wg-default element
    const weglotContainer = document.querySelector('.weglot-container.wg-default');

    // Select the .weglot--w.is--mobile-nav element
    const mobileNav = document.querySelector('.weglot--w.is--mobile-nav');

    // Check the screen width
    if (window.innerWidth <= 991) {
      // Append the .weglot-container.wg-default to the .weglot--w.is--mobile-nav if both elements exist
      if (weglotContainer && mobileNav) {
        mobileNav.appendChild(weglotContainer);
      } else {
        console.error('Either .weglot-container.wg-default or .weglot--w.is--mobile-nav element is not found.');
      }
    } else {
      // Optionally, handle the case for larger screens if necessary
      // console.log('Screen width is greater than 991px.');
    }
  }

  // Initial execution with a timeout to ensure DOM is ready
  setTimeout(handleResponsiveWeglot, 1000); // Adjust the delay if necessary

  // Add a resize event listener to handle screen size changes
  window.addEventListener('resize', handleResponsiveWeglot);
</script>


<!-- popup 
<script>
  // Define the distance
  const distance = "3rem";

  // Function to prevent scrolling
  function scrollStop() {
    document.documentElement.style.overflow = "hidden";
    document.body.style.overflow = "hidden";
  }

  // Function to set initial states and animate to their initial positions
  function initPopup() {
    // Set elements to their end states
    document.querySelector("[data-popup='wrap']").style.display = "flex";
    gsap.set("[data-popup='card']", { y: distance, autoAlpha: 0 });
    gsap.set("[data-popup='close']", { y: distance, autoAlpha: 0 });
    gsap.set("[data-popup='ul']", { autoAlpha: 0 });

    // Animate elements to their initial states
    gsap.to("[data-popup='card']", {
      delay: 0.4,
      y: 0,
      autoAlpha: 1,
      duration: 1,
      ease: "power2.inOut"
    });

    gsap.to("[data-popup='close']", {
      y: 0,
      autoAlpha: 1,
      duration: 1,
      ease: "power2.inOut",
      delay: 0.2
    });

    gsap.to("[data-popup='ul']", {
      autoAlpha: 1,
      duration: 0.5,
      ease: "power2.inOut",
    });
  }

  // Function to handle the animations on close/ul click
  function hidePopup() {
    // Animate the card
    gsap.to("[data-popup='card']", {
      y: distance,
      autoAlpha: 0,
      duration: 1,
      ease: "power2.inOut"
    });

    // Animate the close button
    gsap.to("[data-popup='close']", {
      y: distance,
      autoAlpha: 0,
      duration: 1,
      ease: "power2.inOut",
      delay: 0.2
    });

    // Animate the ul element
    gsap.to("[data-popup='ul']", {
      autoAlpha: 0,
      duration: 0.5,
      ease: "power2.inOut",
      delay: 0.7,
      onComplete: function() {
        // Set wrap to display: none
        document.querySelector("[data-popup='wrap']").style.display = "none";
        // Allow scrolling again
        document.documentElement.style.overflow = "";
        document.body.style.overflow = "";

        // Set localStorage to mark that the popup has been hidden
        localStorage.setItem('hasVisited', 'true');
      }
    });
  }

  // Function to check local storage for user visit
  function checkUserVisit() {
    const hasVisited = localStorage.getItem('hasVisited');
    if (!hasVisited) {
      scrollStop();
      initPopup();
    }
  }

  // Function to clear local storage and reload the page
  function clearLocalStorageAndReload() {
    localStorage.clear();
    location.reload();
  }

  // Run the check on page load
  checkUserVisit();

  // Add event listeners to the close and ul elements
  document.querySelector("[data-popup='close']").addEventListener("click", hidePopup);
  document.querySelector("[data-popup='ul']").addEventListener("click", hidePopup);
</script>
