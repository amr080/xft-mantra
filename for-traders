<script>
  // ------------------------------------ the function
  // mantra section scroll
  function initSectionScroll() {
    const container = document.querySelector('[data-mc="container"]');
    const items = document.querySelectorAll('[data-mc="item"]');

    items.forEach((item, index) => {
      const content = item.querySelector('[data-mc="item-content"]');
      gsap.set(content, { scale: 1, filter: "blur(0px)", y: 0 });

      ScrollTrigger.create({
        trigger: item,
        start: "top top",
        end: "bottom top",
        onEnter: () => {
          if (index > 0) {
            const prevContent = items[index - 1].querySelector(
              '[data-mc="item-content"]',
            );
            const scale = 0.95;
            const yOffset = "-2rem";
            gsap.to(prevContent, {
              scale: scale,
              filter: "blur(1px)",
              y: yOffset,
              duration: 0.3,
            });
          }
        },
        onEnterBack: () => {
          if (index > 0) {
            const prevContent = items[index - 1].querySelector(
              '[data-mc="item-content"]',
            );
            const scale = 0.95;
            const yOffset = "-2rem";
            gsap.to(prevContent, {
              scale: scale,
              filter: "blur(1px)",
              y: yOffset,
              duration: 0.3,
            });
          }
        },
        onLeave: () => {
          if (index < items.length - 1) {
            gsap.to(content, {
              scale: 1,
              filter: "blur(0px)",
              y: 0,
              duration: 0.3,
            });
          }

          for (let i = 0; i < index; i++) {
            const prevContent = items[i].querySelector(
              '[data-mc="item-content"]',
            );
            const scale = 0.95 - (index - i) * 0.05;
            const yOffset = `-${(index - i + 1) * 2}rem`;
            gsap.to(prevContent, {
              scale: scale,
              filter: "blur(1px)",
              y: yOffset,
              duration: 0.3,
            });
          }
        },
        onLeaveBack: () => {
          gsap.to(content, {
            scale: 1,
            filter: "blur(0px)",
            y: 0,
            duration: 0.3,
          });

          if (index > 0) {
            const prevContent = items[index - 1].querySelector(
              '[data-mc="item-content"]',
            );
            gsap.to(prevContent, {
              scale: 1,
              filter: "blur(0px)",
              y: 0,
              duration: 0.3,
            });
          }
        },
      });
    });
  }

  // do something after resize
  function callAfterResize(func, delay) {
    let dc = gsap.delayedCall(delay || 0.2, func).pause();
    const handler = () => dc.restart(true);
    window.addEventListener("resize", handler);
    return handler;
  }

  // ------------------------------------ launch control
  // call all functions and account for resizing
  document.addEventListener("DOMContentLoaded", (event) => {
    gsap.registerPlugin(ScrollTrigger);

    // section scroll function
    initSectionScroll();

    // refresh ScrollTrigger instances after a resize event
    const refreshScrollTrigger = () => {
      ScrollTrigger.refresh();
    };

    let st = ScrollTrigger.create({
      trigger: ".stick-cards__layout",
      pin: ".sticky-cards__heading",
      pinSpacing: false,
      start: "top top",
      end: "bottom bottom",
    });

    // call the refresh function after a resize event with a slight delay
    callAfterResize(refreshScrollTrigger, 0.2);

    // marquee
    let tl = gsap.timeline({ repeat: -1 });
    tl.fromTo(
      ".marquee_track",
      {
        xPercent: 0,
      },
      {
        xPercent: -50,
        duration: 14,
        ease: "none",
      },
    );

  });
</script>
</body></html>